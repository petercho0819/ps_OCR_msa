  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:auth": "nest build auth",
    "build:company": "nest build company",
    "build:receipt": "nest build receipt",
    "build:notification": "nest build notification",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "nest build auth && nest build company && nest build receipt && nest build notification && node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/ps-ocr-microservice/test/jest-e2e.json"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.613.0",
