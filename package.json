    "@types/multer": "^1.4.11",
    "@types/nodemailer": "^6.4.16",
    "amqp-connection-manager": "^4.1.14",
    "amqplib": "^0.10.4",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.6",
    "exceljs": "^4.4.0",
    "handlebars": "^4.7.8",
    "ioredis": "^5.4.1",
    "joi": "^17.13.3",
    "moment": "^2.30.1",
    "mongoose": "^8.4.5",
    "nestjs-pino": "^4.1.0",
    "nodemailer": "^6.9.15",
    "nodemailer-mailgun-transport": "^2.1.5",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
